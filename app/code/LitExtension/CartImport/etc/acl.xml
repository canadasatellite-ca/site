<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="LitExtension_Core::le">
                    <resource id="LitExtension_CartImport::leci" title="LitExtension" sortOrder="20">
                        <resource id="LitExtension_CartImport::leci_process" title="Cart Import" sortOrder="40" />
                        <resource id="LitExtension_CartImport::leci_config" title="Settings" sortOrder="50" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="LitExtension_CartImport::system_config" title="Cart Import Section" sortOrder="50" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
