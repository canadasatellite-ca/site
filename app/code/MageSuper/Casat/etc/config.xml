<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <freeshippingcustom>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <cutoff_cost>50</cutoff_cost>
                <model>MageSuper\Casat\Model\Carrier\Freeshipping</model>
                <name>Free</name>
                <title>Free Shipping</title>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
            </freeshippingcustom>
            <instorepickup>
                <active>1</active>
                <sallowspecific>0</sallowspecific>
                <cutoff_cost>50</cutoff_cost>
                <model>MageSuper\Casat\Model\Carrier\Instorepickup</model>
                <name>In store pickup</name>
                <title>Pickup</title>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
            </instorepickup>
            <fedex>
                <model>MageSuper\Casat\Model\Carrier\Fedex</model>
            </fedex>
        </carriers>
    </default>
</config>
