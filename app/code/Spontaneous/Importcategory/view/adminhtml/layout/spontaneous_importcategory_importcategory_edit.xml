<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Spontaneous\Importcategory\Block\Adminhtml\Importcategory\Edit" name="spontaneous_importcategory_category_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Spontaneous\Importcategory\Block\Adminhtml\Importcategory\Edit\Tabs" name="spontaneous_importcategory_category_tabs">
                <block class="Spontaneous\Importcategory\Block\Adminhtml\Importcategory\Edit\Tab\Importcategory" name="spontaneous_importcategory_importcategory_edit_tab"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">importcategories</argument>
                    <argument name="block" xsi:type="string">spontaneous_importcategory_importcategory_edit_tab</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>
