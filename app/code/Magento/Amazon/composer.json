{
    "name": "magento/module-amazon",
    "description": "Provides Amazon Integration.",
    "type": "magento2-module",
    "version": "4.3.0",
    "license": [
      "OSL-3.0",
      "AFL-3.0"
    ],
    "require": {
      "php": ">=7.1.3 <7.5.0",
      "ext-json": "*",
      "magento/module-backend": "~101.0 || ~102.0",
      "magento/module-catalog": "~103.0 || ~104.0",
      "magento/module-catalog-import-export": "~101.0",
      "magento/module-catalog-inventory": "~100.3",
      "magento/module-catalog-rule": "~101.1",
      "magento/module-customer": "~102.0 || ~103.0",
      "magento/module-directory": "~100.2",
      "magento/module-eav": "~102.0",
      "magento/module-payment": "~100.3",
      "magento/module-quote": "~101.1",
      "magento/module-rule": "~100.3",
      "magento/module-sales": "~102.0 || ~103.0",
      "magento/module-shipping": "~100.3",
      "magento/module-store": "~101.0",
      "magento/module-tax": "~100.3",
      "magento/module-ui": "~101.1",
      "magento/framework": "~102.0 || ~103.0",
      "magento/services-connector": " >= 1.0.0 < 1.0.6 || ~1.0.7",
      "webonyx/graphql-php": ">=0.12 <0.15.0",
      "beberlei/assert": "~2.4 || ~3.2"
    },
    "suggest": {
      "magento/inventory-composer-metapackage": "To support Multi-source Inventory"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Magento\\Amazon\\": ""
        }
    },
  "archive": {
    "exclude": [
      "/dev/",
      "/.*",
      "/*.zip"
    ]
  }
}
