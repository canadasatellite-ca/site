                <item name="magentoSettings" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Query\MagentoSettings\Proxy</item>
            </argument>
            <argument name="fields" xsi:type="array">
                <item name="Store.irpUrl" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreIrpUrl\Proxy</item>
                <item name="Store.orders" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreOrders\Proxy</item>
                <item name="StoreOrder.orderItems" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreOrderItems\Proxy</item>
                <item name="StoreOrder.orderTracking" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreOrderTracking\Proxy</item>
                <item name="Store.listings" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreListings\Proxy</item>
                <item name="StoreReport.revenue" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreReportRevenue\Proxy</item>
                <item name="StoreReport.lifetime" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Field\StoreReportLifetime\Proxy</item>
            </argument>
            <argument name="mutations" xsi:type="array">
                <item name="createStore" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Mutation\CreateStore\Proxy</item>
                <item name="deleteStore" xsi:type="object">Magento\Amazon\GraphQl\Resolver\Mutation\DeleteStore\Proxy</item>
