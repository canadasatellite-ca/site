    <type name="\Magento\Amazon\Comm\Amazon\UpdateHandler">
        <arguments>
            <argument name="handlers" xsi:type="array">
                <!-- Order matters here. You cannot save attribute value if attribute does not exist yet, etc. -->
                <item name="RemoveDefect" sortOrder="100" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\RemoveDefect</item>
                <item name="RemoveListing" sortOrder="200" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\RemoveListing</item>
                <item name="Listing" sortOrder="300" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Listing</item>
                <item name="Defect" sortOrder="400" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Defect</item>
                <item name="Attribute" sortOrder="500" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Attribute</item>
                <item name="AttributeValue" sortOrder="600" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\AttributeValue</item>
                <item name="BbbPrice" sortOrder="700" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\BbbPrice</item>
                <item name="LowestPrice" sortOrder="800" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\LowestPrice</item>
                <item name="Error" sortOrder="900" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Error</item>
                <item name="Log" sortOrder="1000" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Log</item>
                <item name="Multiple" sortOrder="1100" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Multiple</item>
                <item name="ListStatus" sortOrder="1200" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\ListStatus</item>
                <item name="Variant" sortOrder="1300" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Variant</item>
                <item name="Order" sortOrder="1400" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\Order</item>
                <item name="OrderItem" sortOrder="1500" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\OrderItem</item>
                <item name="ReportRun" sortOrder="1600" xsi:type="object">\Magento\Amazon\Comm\Amazon\UpdateHandler\ReportRun</item>
            </argument>
        </arguments>
    </type>

