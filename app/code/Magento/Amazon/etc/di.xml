                <item name="introspection" xsi:type="object">\Magento\Amazon\Logger\Processor\AddIntrospection</item>
                <item name="psr_log_message" xsi:type="object">\Monolog\Processor\PsrLogMessageProcessor</item>
                <item name="exception_stacktrace" xsi:type="object">\Magento\Amazon\Logger\Processor\AddStackTraceToException</item>
                <item name="errors_for_aggregate_exception" xsi:type="object">\Magento\Amazon\Logger\Processor\AddErrorsForAggregatedException</item>
                <item name="http_response_for_api_exceptions" xsi:type="object">\Magento\Amazon\Logger\Processor\LogHttpResponseForApiExceptions</item>
                <item name="previous_exception" xsi:type="object">\Magento\Amazon\Logger\Processor\LogPreviousException</item>
                <item name="trim_path" xsi:type="object">\Magento\Amazon\Logger\Processor\TrimMagentoBaseDirFromLog</item>
                <item name="cleanup_objects" xsi:type="object">\Magento\Amazon\Logger\Processor\CleanUpObjectsFromLogs</item>
            </argument>
        </arguments>
    </type>

