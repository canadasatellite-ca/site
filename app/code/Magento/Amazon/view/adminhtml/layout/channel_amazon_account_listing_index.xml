<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Magento_Amazon::css/source/module.css"/>
        <css src="Magento_Amazon::css/source/spectrum.css"/>
    </head>
    <body>
        <container name="analytics" after="content">
            <block class="Magento\Amazon\Block\Adminhtml\Analytics" name="adobe_analytics" template="Magento_Amazon::analytics.phtml"/>
        </container>
        <referenceContainer name="admin.scope.col.wrap" htmlClass="admin__old" />
        <referenceContainer name="page.main.actions">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\ActionButtons" name="setup" template="Magento_Amazon::amazon/account/buttons.phtml">
                <arguments>
                    <argument name="current" xsi:type="string">listings_list</argument>
                </arguments>
            </block>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\ValidatingAsinCount" name="listing_validatingasin_count" template="Magento_Amazon::amazon/account/listing/validatingasincount.phtml"/>
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View" name="amazon_account_listing_grid">
                <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tabs" name="amazon_account_listing_edit_tabs">
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Incomplete" name="listing_view_incomplete.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_incomplete</argument>
                        <argument name="block" xsi:type="string">listing_view_incomplete.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Thirdparty" name="listing_view_thirdparty.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_thirdparty</argument>
                        <argument name="block" xsi:type="string">listing_view_thirdparty.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Published" name="listing_view_published.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_published</argument>
                        <argument name="block" xsi:type="string">listing_view_published.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Inactive" name="listing_view_inactive.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_inactive</argument>
                        <argument name="block" xsi:type="string">listing_view_inactive.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Active" name="listing_view_active.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_active</argument>
                        <argument name="block" xsi:type="string">listing_view_active.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Overrides" name="listing_view_overrides.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_overrides</argument>
                        <argument name="block" xsi:type="string">listing_view_overrides.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Ineligible" name="listing_view_ineligible.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_ineligible</argument>
                        <argument name="block" xsi:type="string">listing_view_ineligible.grid.container</argument>
                    </action>
                    <block class="Magento\Amazon\Block\Adminhtml\Amazon\Account\Listing\View\Tab\Ended" name="listing_view_ended.grid.container"/>
                    <action method="addTab">
                        <argument name="name" xsi:type="string">listing_view_ended</argument>
                        <argument name="block" xsi:type="string">listing_view_ended.grid.container</argument>
                    </action>
                </block>
            </block>
        </referenceContainer>
        <referenceBlock name="listing_view_active.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_active_grid" template="Magento_Amazon::amazon/account/listing/active.phtml"/>
            <uiComponent name="amazon_listing_active_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_inactive.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_inactive_grid" template="Magento_Amazon::amazon/account/listing/inactive.phtml"/>
            <uiComponent name="amazon_listing_inactive_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_incomplete.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_incomplete_grid" template="Magento_Amazon::amazon/account/listing/incomplete.phtml"/>
            <uiComponent name="amazon_listing_incomplete_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_published.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_published_grid" template="Magento_Amazon::amazon/account/listing/published.phtml"/>
            <uiComponent name="amazon_listing_published_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_thirdparty.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_thirdparty_grid" template="Magento_Amazon::amazon/account/listing/thirdparty.phtml"/>
            <uiComponent name="amazon_listing_thirdparty_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_ended.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_ended_grid" template="Magento_Amazon::amazon/account/listing/ended.phtml"/>
            <uiComponent name="amazon_listing_ended_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_ineligible.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_eligible_grid" template="Magento_Amazon::amazon/account/listing/ineligible.phtml"/>
            <uiComponent name="amazon_listing_ineligible_grid"/>
        </referenceBlock>
        <referenceBlock name="listing_view_overrides.grid.container">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_overrides_grid" template="Magento_Amazon::amazon/account/listing/overrides.phtml"/>
            <uiComponent name="amazon_listing_overrides_grid"/>
        </referenceBlock>
    </body>
</page>
