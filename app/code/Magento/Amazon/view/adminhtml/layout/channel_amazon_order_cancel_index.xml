<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Magento_Amazon::css/source/module.css"/>
        <css src="Magento_Amazon::css/source/spectrum.css"/>
    </head>
    <body>
        <container name="analytics" after="content">
            <block class="Magento\Amazon\Block\Adminhtml\Analytics" name="adobe_analytics" template="Magento_Amazon::analytics.phtml"/>
        </container>
        <referenceContainer name="admin.scope.col.wrap" htmlClass="admin__old" /><referenceContainer name="content">
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\Order\Cancel\View" name="orders_cancel_view_form"/>
            <block class="Magento\Amazon\Block\Adminhtml\Amazon\General" name="amazon_general_cancel" template="Magento_Amazon::amazon/order/cancel/cancel.phtml"/>
            <uiComponent name="amazon_order_cancel_form"/>
        </referenceContainer>
    </body>
</page>
