<?xml version="1.0"?>
<page layout="2columns-left" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">   
    <update handle="customer_account"/>
    <head>
        <title>Credit Cards</title>
        <css src="Magedelight_Firstdata::css/firstdata.css"/>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Credit Cards</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="CanadaSatellite\Theme\Block\Customer\Card\ListCard" name="card-list" template="customer/card/listing.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
