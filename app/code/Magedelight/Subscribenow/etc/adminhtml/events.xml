<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="catalog_product_save_before">
        <observer name="md_subscription_product_save_before" instance="Magedelight\Subscribenow\Observer\ProductSaveBefore" />
    </event>     
    <event name="admin_system_config_changed_section_md_subscribenow">
        <observer name="magedelight_subscribenow_admin_system_config_changed_section_md_subscribenow" instance="Magedelight\Subscribenow\Observer\Mdkc"/>
        <observer name="magedelight_subscribenow_frd_track" instance="Magedelight\Subscribenow\Observer\Mdfrd" />
    </event>     
    <event name="md_subscribenow_system_config_changed">
        <observer name="magedelight_subscribenow_admin_system_config_changed_section_md_subscribenow" instance="Magedelight\Subscribenow\Observer\Mdkc"/>
        <observer name="magedelight_subscribenow_frd_track" instance="Magedelight\Subscribenow\Observer\Mdfrd" />
    </event>    
</config>
