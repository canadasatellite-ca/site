<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="footer_links">
            <block ifconfig ='md_faq/general/enabled_faq' class="Magento\Framework\View\Element\Html\Link\Current" name="md-faq-link">
                <arguments>
                    <argument name="label" xsi:type="helper" helper="Magedelight\Faqs\Helper\Data::getFaqTitle"></argument>
                    <argument name="path" xsi:type="helper" helper="Magedelight\Faqs\Helper\Data::getFaqUrl"></argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
